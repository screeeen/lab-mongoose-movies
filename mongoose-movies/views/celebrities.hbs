<h1>Celebrities</h1>

{{#each allTheCelebritiesFromDB}}
<p class='celebrities-title'>
    {{!-- <a href="/celebrities/edit?_id={{this._id}}" class="edit-button">EDIT</a> --}}
    <a href="/celebrities/{{this._id}}">{{this.name}}</a>
    <a href="/celebrities/{{this._id}}">{{this.occupation}}</a>
    <a href="/celebrities/{{this._id}}">{{this.catchPhrase}}</a>
    <form action="/:id/delete" method="post">
          <button type="submit">DELETE</button>

</p>
{{/each}}